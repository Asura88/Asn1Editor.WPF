<Window x:Class="SysadminsLV.Asn1Editor.Views.Windows.LicenseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="License" Height="450" Width="800">
    <DockPanel>
        <DockPanel DockPanel.Dock="Bottom" Margin="0,5,0,0" Background="{StaticResource PanelHeaderBackground}">
            <Button Content="Close" Click="CloseClick" HorizontalAlignment="Right" Width="60" Margin="5"/>
        </DockPanel>
        <RichTextBox>
            <RichTextBox.Document>
                <FlowDocument>
                    <Paragraph>
                        <Run></Run>
                    </Paragraph>
                </FlowDocument>
            </RichTextBox.Document>
        </RichTextBox>
    </DockPanel>
</Window>
